#VRML V2.0 utf8

#Piece d'entree
#Contient 6 torches sur les murs

Inline	#Murs
{
	url "murs.wrl"
}#/Murs

#Torches
Transform #Trio de torches
{
	translation 0 0 -3
	children DEF TrioTorches Transform
	{
		translation -3 1.5 0
		children 
		[
			 DEF Torche Transform
			{
				rotation 0 1 0 1.57
				children Inline #Torche milieu
				{
					url "../Torche/Torche.wrl"
				}
			}
					
			Transform
			{
				translation 0 0 -1.5 #Torche droite
				children USE Torche
			}
			Transform
			{
				translation 0 0 1.5 #Torche gauche
				children USE Torche
			}
		]
	}

}
	#/Trio de torches

Transform
{
	translation 0 0 -3
	rotation 0 1 0 3.1416
	children USE TrioTorches
}
#/Torches


#Statuettes de chat
Transform
{
	translation 0 0 -3
	rotation 0 1 0 .78
	children
	[
		DEF StatueChat Transform 
		{
			scale .2 .2 .2
			translation 0 0 -3
			children Inline
			{
				url "../StatuetteChat/StatuetteChat.wrl"
			}
		}

		Transform
		{
			rotation 0 1 0 1.57
			children USE StatueChat
		}

		Transform
		{
			rotation 0 1 0 3.14
			children USE StatueChat
		}

		Transform
		{
			rotation 0 1 0 -1.57
			children USE StatueChat
		}
	]
}
#/Statuettes de chat
	



